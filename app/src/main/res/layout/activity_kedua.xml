<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    android:orientation="vertical"
    tools:context="com.oji.ajinurlaksono_1202150032_modul1.kedua">

    <TextView
        android:id="@+id/textView6"
        android:textSize="@dimen/headSize"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Makan Malam"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="26dp"
        android:layout_marginLeft="98dp"
        app:layout_constraintLeft_toLeftOf="parent" />
    <TextView
        android:id="@+id/tTempat"
        android:textSize="@dimen/normalSize"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:layout_marginTop="13dp"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        android:layout_marginLeft="151dp"
        app:layout_constraintLeft_toLeftOf="parent" />
    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:textSize="@dimen/head2Size"
        android:layout_height="wrap_content"
        android:text="Porsi"
        android:layout_marginTop="32dp"
        app:layout_constraintTop_toBottomOf="@+id/tMenu"
        android:layout_marginLeft="175dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/tJumlah"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="187dp"
        android:layout_marginTop="20dp"
        android:gravity="center_vertical"
        android:text="2"
        android:textSize="@dimen/normalSize"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />


    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="173dp"
        android:layout_marginTop="30dp"
        android:text="Menu"
        android:textSize="@dimen/head2Size"
        app:layout_constraintTop_toBottomOf="@+id/tTempat" />

    <TextView
        android:id="@+id/tMenu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="151dp"
        android:layout_marginTop="20dp"
        android:text="TextView"
        android:textSize="@dimen/normalSize"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />


    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="172dp"
        android:layout_marginTop="36dp"
        android:text="Harga"
        android:textSize="@dimen/head2Size"

        app:layout_constraintTop_toBottomOf="@+id/tJumlah" />

    <TextView
        android:id="@+id/tHarga"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textSize="@dimen/normalSize"
        android:layout_marginTop="21dp"
        app:layout_constraintTop_toBottomOf="@+id/textView9"
        android:layout_marginLeft="161dp"
         />
</LinearLayout>
